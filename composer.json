{
    "name": "bigenergy/laravel-freekassa-ru",
    "description": "freekassa.ru payments for Laravel 7.0+",
    "license": "MIT",
    "authors": [
        {
            "name": "weishaypt",
            "email": "ya@sanek.dev"
        },
        {
            "name": "bigenergy",
            "email": "bigenergyyy@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": "9.*",
        "guzzlehttp/guzzle": "~6.2"
    },
    "autoload": {
        "psr-4": {
            "Weishaypt\\FreeKassa\\Facades\\": "src/Facades",
            "Weishaypt\\FreeKassa\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Weishaypt\\FreeKassa\\FreeKassaServiceProvider"
            ],
            "aliases": {
                "FreeKassa": "Weishaypt\\FreeKassa\\Facades\\FreeKassa"
            }
        }
    }
}
